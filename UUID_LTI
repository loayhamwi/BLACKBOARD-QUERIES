SELECT
distinct
U.USER_ID,
U.FIRSTNAME,
U.LASTNAME,
CM.COURSE_ID,
concat(U.UUID,'@lms.com')

FROM 
           BBLEARN.USERS U 
INNER JOIN BBLEARN.COURSE_USERS CU ON U.PK1=CU.USERS_PK1
INNER JOIN BBLEARN.COURSE_MAIN CM ON CU.CRSMAIN_PK1=CM.PK1
INNER JOIN BBLEARN.DATA_SOURCE DS ON DS.PK1=CU.DATA_SRC_PK1
WHERE 
(CM.course_id like 'ENG-001-%-202230' or CM.course_id like 'ENG-003-%-202230')
AND CU.ROLE='S' and user_id not like '%_previewuser'
;
