--GET ORGANIZATIONS ENROLLMENTS OF NON BANNER  JUST CHANGE THE TERM CODE
select 
   CM.COURSE_ID as EXTERNAL_ORGANIZATION_KEY,
    U.BATCH_UID AS EXTERNAL_PERSON_KEY,
    CU.ROLE,
    CU.AVAILABLE_IND,
    'ENABLED'AS ROW_STATUS,
    '202110_ORGANIZATIONS_ENROLLMENTS' AS DATA_SOURCE_KEY
FROM       BBLEARN.USERS U
INNER JOIN BBLEARN.COURSE_USERS CU ON U.PK1=CU.USERS_PK1
INNER JOIN BBLEARN.COURSE_MAIN CM ON CU.CRSMAIN_PK1=CM.PK1
WHERE 
CM.COURSE_ID 
IN
(
'BU-FACULTY-ACCOUNTING-202020',
'BU-FACULTY-COMMITTEE-ACTIVITY-202020',
'BU-FACULTY-COMMITTEE-STUDENTRIGHTS-202020',
'BU-FACULTY-ECOMM-202020',
'CI-FACULTY-CS-202020',
'CI-FACULTY-IT-202020',
'CI-FACULTY-PROFESSIONAL-TRAINING-202020',
'HS-RESEARCH-202020',
'SA-FACULTY-BASICSCIENCES-202020',
'SA-FACULTY-HUMANITIES-202020',
'SA-FACULTY-LAW-202020'--,
--'HS-STUDENT-F-202020',
--'HS-STUDENT-M-202020',
--'PY-FACULTY-202020',
--'PY-STUDENT-F-202020',
--'PY-STUDENT-M-202020',
--'SA-FACULTY-202020',
--'CI-STUDENT-F-202020',
--'CI-STUDENT-M-202020',
--'HS-FACULTY-202020',
--'HS-FACULTY-PUBLICHEALTH-202020',
--'BU-STUDENT-F-202020',
--'BU-STUDENT-M-202020',
--'CI-FACULTY-202020',
--'BU-FACULTY-202020',
--'SA-STUDENT-F-202020',
--'SA-STUDENT-M-202020'
);
