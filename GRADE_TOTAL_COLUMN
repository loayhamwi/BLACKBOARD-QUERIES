
SELECT     GGC.COURSE_USERS_PK1 AS ENROLMENTS_ID,
           GGC.SCORE AS TOTAL,
           GGC.POSSIBLE
FROM       BBLEARN.GRADEBOOK_GRADE_CALC GGC 
INNER JOIN BBLEARN.GRADEBOOK_MAIN GM ON GM.PK1=GGC.GRADEBOOK_MAIN_PK1 
INNER JOIN BBLEARN.COURSE_MAIN CM ON CM.PK1=GM.CRSMAIN_PK1
INNER JOIN BBLEARN.DATA_SOURCE DS ON DS.PK1=CM.DATA_SRC_PK1
WHERE 
GM.CALCULATED_IND='T' AND
CM.COURSE_ID ='ACCT-490-30170-201930' ;
;
