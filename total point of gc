select    
distinct u.FIRSTNAME,u.LASTNAME,u.user_id,cm.course_id,cm.COURSE_NAME,gm.TITLE,GGC.SCORE as CURRENT_GRADE,           
                   GGC.POSSIBLE AS POSSIBLE_GRADE, 'Y' as LATEST_IND, 'TOTAL COLUMN' AS ATTEMPT_STATUS   
   
from BBLEARN.GRADEBOOK_GRADE_CALC GGC      
JOIN  BBLEARN.GRADEBOOK_MAIN GM ON GM.PK1=GGC.GRADEBOOK_MAIN_PK1     
JOIN BBLEARN.COURSE_USERS CU ON CU.PK1=GGC.COURSE_USERS_PK1     
JOIN BBLEARN.COURSE_MAIN CM ON CM.PK1=CU.CRSMAIN_PK1     
JOIN BBLEARN.USERS U ON U.PK1=CU.USERS_PK1     
where      

  (cm.course_id like 'ENG-001-%-202110' OR  cm.course_id like 'ENG-003-%-202110')    
  and GM.CALCULATED_IND='T'  AND    
CU.ROLE='S' 
AND  
U.DATA_SRC_PK1!='496';
